@model Vidly.ViewModels.AllMoviesViewModel
@{
    ViewBag.Title = "Movies";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Movies</h2>
<table class="table table-striped table-hover">
    <thead>
    <tr>
        <th>Title</th>  
    </tr>
    </thead>
    <tbody>
        @foreach (var movie in Model.Movies)
        {
        <tr>
            <td>@movie.Name</td>
        </tr>
        }
    </tbody>
</table>

